<h1>注文履歴詳細</h1>
 <%= form_with model:@customer, url: admin_customer_path, method: :post  do |f| %>

 <h4>購入者</h4>
  <%= f.text_field : %>

  <h4>注文日</h4>
  <%= f.text_area :last_name, :first_name %>

  <h4>配送先</h4>
  <%= f.text_field :address %>
  
  <h4>支払方法</h4>
  <%= f.text_field :postal_code %>

  <h4>商品名</h4>
  <%= f.text_field :telephone_number %>

  <h4>単価</h4>
  <%= f.text_field :email %>

  <h4>数量</h4>
  <%= f.text_field :customer_id %>
  
  <h4>小計</h4>
  <%= f.text_field :address %>
  
  <h4>商品合計</h4>
  
  <h4>送料</h4>
  
  <h4>請求金額合計</h4>
  
  <% end %>